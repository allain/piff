fn println(msg) {
  print(msg + "\n")
}

println("1 + 2 === " + (1 + 2))
println("1 + (2 * 3) === " + (1 + (2 * 3)))
println("(1 + 2) * 3 === " + ((1 + 2) * 3))

// logical operators
println("1 || 2 === " + (1 || 2))
println("false && true === " + ((false && true) ? "true" : "false"))

println("1" + "2" + "3")

println(true && true)