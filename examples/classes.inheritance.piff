fn println(msg) {
  print(msg + "\n")
}

class Person {
  first = ""
  last = ""

  Person(first, last) {
    @first = first
    @last = last
  }

  fullName() {
    return @first + " " + @last
  }
}

class Adult extends Person {
  title = ""

  Adult(title, first, name) {
    parent(first, name)
    @title = title
  }

  fullName() {
    return @title + " " + parent()
  }
}

adult = new Adult("Mr", "Popo", "Jijo")
println(adult.fullName)
