// simple function
add = fn (a, b) {
  return a + b
}

fn div(n, d) {
  return n / d
}

// anonymous function
multiply = fn (a, b) {
  return a * b
}

// anonymous function using external variables
ticks = []

tick = fn () {
  // Woohoo!, no need to declare ticks as being used by this closure
  ticks[] = mktime()
}
